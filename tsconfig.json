{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
        "jsx": "react",
        "allowJs": true,
        "checkJs": false,
        "target": "es6",
        "module": "commonjs",
        "strict": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
  }
}
